---
// component imports
import Button from "../components/Button.astro";
import Header from "../components/Header.astro";
import Layout from "../layouts/Layout.astro";
import Footer from "../components/Footer.astro";

import "../styles/pages/about.scss";

// Asset imports
import aboutImg from "../assets/about-hero.png";
import companyStructure from "../assets/company-structure.png";
import valuesImg from "../assets/values-img.png";
import blob from "../assets/blurry-blob.svg";
---

<Layout title="PactZion - About us">
  <Header />
  <main>
    <section class="about-hero-section">
      <div class="wrapper container">
        <div class="hero-header">
          <div class="headline">
            <p class="section-label">About Us</p>
            <h1 class="fw-semi-bold-100">
              We are your <span style="color: #EEAA44">trusted</span> trade partner,
              based in Ethiopia
            </h1>
          </div>

          <div class="subheader">
            <p class="fw-bold subheader-title">
              PactZion is dedicated to crafting innovative solutions that
              revolutionize the world of commerce.
            </p>
            <p class="subheader-description">
              With a focus on integrity and innovation, we empower businesses to
              thrive in the global marketplace, fostering sustainable growth and
              mutual success
            </p>
          </div>
        </div>
      </div>
    </section>

    <section class="about-img-section">
      <div class="container wrapper | about-img-box">
        <img src={aboutImg.src} alt="Pactzion staff members" />
        <div class="about-pactzion">
          <p class="about-text fw-bold">
            With a focus on integrity and innovation, we empower businesses to
            thrive in the global marketplace, fostering sustainable growth and
            mutual success
          </p>
          <Button text="Get in touch" class="btn-tertiary btn-dark" />
        </div>
      </div>
    </section>

    <section class="mission-vision-section">
      <div class="wrapper container">
        <div class="boxes-wrapper">
          <div class="mission-box box">
            <h2 class="fs-tertiary-header fw-semi-bold-100">Our Mission</h2>
            <p>
              Moving towards the goal of becoming the most trusted and reliable
              resource in identifying the right commodities and the right trade
              partners for both our users and suppliers.
            </p>
          </div>
          <div class="vision-box box">
            <h2 class="fs-tertiary-header fw-semi-bold-100">Our Vision</h2>
            <p>
              We strive to generate value to our customers, employees, the
              community and the nation at large.
            </p>
          </div>
        </div>
      </div>
    </section>
    <section class="values-section">
      <div class="wrapper container">
        <h2 class="fw-semi-bold-200 fs-secondary-header">Our Core Values</h2>
